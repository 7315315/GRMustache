{
  "tests": [
    {
      "name": "Overridable sections, unlike regular sections, render for missing values",
      "data": { },
      "template": "<{{$overridable}}[{{subject}}]{{/overridable}}>",
      "expected": "<[]>"
    },
    {
      "name": "Overridable sections do not render for false keys",
      "data": { "overridable": false },
      "template": "<{{$overridable}}[{{subject}}]{{/overridable}}>",
      "expected": "<>"
    },
    {
      "name": "Overridable sections do not render for empty lists",
      "data": { "overridable": [] },
      "template": "<{{$overridable}}[{{subject}}]{{/overridable}}>",
      "expected": "<>"
    },
    {
      "name": "Overridable sections open new contexts",
      "data": { "overridable": { "subject": "---" } },
      "template": "<{{$overridable}}[{{subject}}]{{/overridable}}>",
      "expected": "<[---]>"
    }
  ]
}
